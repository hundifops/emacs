#include <stdio.h>
/* defines an array of 10 integers */
int numbers = [10];

/* populate the array */

numbers[0] = 10;
numbers[1] = 20;
numbers[2] = 30;
numbers[3] = 40;
numbers[4] = 50;
numbers[5] = 60;
numbers[6] = 70;
numbers[7] = 80;

/* indexing starts at 0. print the 7th number from the array */

printf("the 7th number in the array is", array[6]);

int main() {
  /* define variables */
  int grades[3];
  int average;

  grades[0] = 80;
  grades[1] = 90;
  grades[2] = 85;

  average = (grades[0] + grades[1] + grades[2]) / 3;
  printf("The average of the 3 grades is", average);

  return 0;
}
